#include <stdio.h>

void celsiusToOthers(float celsius) {
    float fahrenheit = (celsius * 9 / 5) + 32;
    float kelvin = celsius + 273.15;
    printf("Fahrenheit: %.2f\n", fahrenheit);
    printf("Kelvin: %.2f\n", kelvin);
}

void fahrenheitToOthers(float fahrenheit) {
    float celsius = (fahrenheit - 32) * 5 / 9;
    float kelvin = celsius + 273.15;
    printf("Celsius: %.2f\n", celsius);
    printf("Kelvin: %.2f\n", kelvin);
}

void kelvinToOthers(float kelvin) {
    float celsius = kelvin - 273.15;
    float fahrenheit = (celsius * 9 / 5) + 32;
    printf("Celsius: %.2f\n", celsius);
    printf("Fahrenheit: %.2f\n", fahrenheit);
}

int main() {
    int choice;
    float temp;

    printf("Temperature Converter\n");
    printf("1. Celsius to Fahrenheit and Kelvin\n");
    printf("2. Fahrenheit to Celsius and Kelvin\n");
    printf("3. Kelvin to Celsius and Fahrenheit\n");
    printf("Enter your choice (1-3): ");
    scanf("%d", &choice);

    if (choice >= 1 && choice <= 3) {
        printf("Enter the temperature to convert: ");
        scanf("%f", &temp);

        switch (choice) {
            case 1:
                celsiusToOthers(temp);
                break;
            case 2:
                fahrenheitToOthers(temp);
                break;
            case 3:
                kelvinToOthers(temp);
                break;
        }
    } else {
        printf("Invalid choice.\n");
    }

    return 0;
}
